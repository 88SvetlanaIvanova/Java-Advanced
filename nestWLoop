public class nestWLoop {
    public static void main(String[] args) {
        int i = 9;

        while (i >= 0){
            int j = i;
            if (i == 6 ){
                continue;
            }
            while(j >= 0){
                if(j == 6) break;
                System.out.print(j + " ");
                j--;
            }

            System.out.println();
            i --;
        }
    }
}
