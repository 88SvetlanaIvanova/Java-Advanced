public class nestLoop {
    public static void main(String[] args) {
        for (int i = 9; i >= 0 ; i--) {
            if (i == 7){
                continue;
            }
            for (int j = i; j >= 0 ; j--) {
                System.out.print(j + " ");
            }
            System.out.println();
        }
    }
}
